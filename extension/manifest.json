{
  "manifest_version": 2,
  "name": "AbBOT",
  "description": "This extension will open https://prolifewhistleblower.com/anonymous-form/ and automatically fill out the form for you. Complete the CAPTCHA, and submit the form. Repeat.",
  "version": "0.0.1",
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "https://prolifewhistleblower.com/anonymous-form/"
      ],
      "js": [
        "extension.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "texas.png"
    }
  },
  "icons": {
    "16": "texas.png",
    "48": "texas.png",
    "128": "texas.png"
  },
  "web_accessible_resources": [
    "src/index.js",
    "src/randomName.js",
    "src/randomText.js",
    "src/randomZip.js",
    "src/vendor/Markov.js"
  ]
}
